{
  "title": "Genomic epidemiology of MERS-CoV",
  "maintainers": [
    {"name": "James Richard Otieno, WHO EPM Emerging Threats Diagnostics team", "url": "http://nextstrain.org"}
  ],
  "data_provenance": [
    {
      "name": "GenBank",
      "url": "https://www.ncbi.nlm.nih.gov/genbank/"
    }
  ],
  "build_url": "https://github.com/nextstrain/mers",
  "colorings": [
    {
      "key": "clade",
      "title": "Clade",
      "type": "categorical",
      "scale": [
        ["A", "#1f77b4"],
        ["B", "#d62728"],
        ["C", "#ff7f0e"]
      ]
    },
    {
      "key": "WHO_region",
      "title": "WHO Region",
      "type": "categorical"
    },
    {
      "key": "country",
      "title": "Country",
      "type": "categorical"
    },
    {
      "key": "host",
      "title": "Host",
      "type": "categorical"
    },
    {
      "key": "date",
      "title": "Collection date",
      "type": "continuous"
    }
  ],
  "geo_resolutions": [
    "country"
  ],
  "display_defaults": {
    "color_by": "clade",
    "map_triplicate": true,
    "branch_label": "clade",
    "distance_measure": "div",
    "transmission_lines": false
  },
  "filters": [
    "clade",
    "host",
    "country",
    "WHO_region"
  ]
}
